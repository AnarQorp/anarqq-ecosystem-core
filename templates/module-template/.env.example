# Application Configuration
NODE_ENV=development
PORT=3000
LOG_LEVEL=debug

# Service Mock Configuration
# Comma-separated list of services to mock (all, none, or specific services)
MOCK_SERVICES=all
# MOCK_SERVICES=squid,qlock
# MOCK_SERVICES=none

# Q Ecosystem Service URLs (for integrated mode)
SQUID_URL=http://localhost:3001
QLOCK_URL=http://localhost:3002
QONSENT_URL=http://localhost:3003
QINDEX_URL=http://localhost:3004
QERBEROS_URL=http://localhost:3005
QMASK_URL=http://localhost:3006

# Infrastructure Services
REDIS_URL=redis://localhost:6379
IPFS_URL=http://localhost:5001

# Security Configuration
JWT_SECRET=your-jwt-secret-here
ENCRYPTION_KEY=your-encryption-key-here
API_KEY=your-api-key-here

# Rate Limiting
RATE_LIMIT_WINDOW_MS=900000
RATE_LIMIT_MAX_REQUESTS=100

# Monitoring and Observability
METRICS_ENABLED=true
TRACING_ENABLED=true
HEALTH_CHECK_INTERVAL=30000

# Module-Specific Configuration
{{MODULE_NAME}}_SPECIFIC_CONFIG=value

# Development Keys (DO NOT USE IN PRODUCTION)
DEV_PRIVATE_KEY=your-dev-private-key-here
DEV_PUBLIC_KEY=your-dev-public-key-here