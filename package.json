{
  "name": "anarq-frontend",
  "version": "1.0.0",
  "type": "module",
  "scripts": {
    "dev": "vite --config frontend-build/vite.config.ts",
    "build": "vite build --config frontend-build/vite.config.ts",
    "build:dev": "vite build --mode development",
    "preview": "vite preview --config frontend-build/vite.config.ts",
    "test:integration": "node scripts/run-integration-tests.mjs",
    "test:integration:watch": "vitest --config vitest.config.ts backend/tests/*integration*.test.mjs",
    "test:integration:coverage": "vitest run --coverage backend/tests/*integration*.test.mjs",
    "test:e2e": "node scripts/run-e2e-tests.mjs",
    "test:e2e:smoke": "node scripts/run-e2e-tests.mjs --suite=smoke",
    "test:e2e:performance": "node scripts/run-e2e-tests.mjs --suite=performance",
    "test:e2e:workflow": "node scripts/run-e2e-tests.mjs --suite=workflow",
    "test:e2e:ci": "node scripts/run-e2e-tests.mjs --fail-fast",
    "test:e2e:watch": "vitest --config vitest.e2e.config.ts --watch",
    "test:e2e:coverage": "vitest run --config vitest.e2e.config.ts --coverage",
    "security:sast": "npm run security:eslint && npm run security:audit",
    "security:eslint": "eslint . --ext .js,.mjs,.ts,.tsx --config .eslintrc.security.js --format json --output-file eslint-security-results.json",
    "security:audit": "npm audit --audit-level=moderate --json > npm-audit-results.json || true",
    "security:deps": "npm run security:audit && npm run security:retire",
    "security:retire": "retire --outputformat json --outputpath retire-results.json --severity medium || true",
    "security:quality-gate": "python3 scripts/security/security-quality-gate.py --config .github/security-gates.yml",
    "security:report": "python3 scripts/security/generate-comprehensive-report.py --results-dir . --output security-report.html",
    "test:coverage": "vitest run --coverage",
    "test:docs-consolidation": "vitest --config vitest.docs-consolidation.config.ts",
