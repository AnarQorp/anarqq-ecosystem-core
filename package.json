    "docs:monitor:cleanup": "node scripts/docs-monitoring-automation-integration.mjs cleanup",
    "docs:kpi:track": "node scripts/docs-kpi-tracker.mjs track",
    "docs:kpi:report": "node scripts/docs-kpi-tracker.mjs report",
    "docs:kpi:json": "node scripts/docs-kpi-tracker.mjs json",
    "docs:dashboard": "node scripts/docs-monitoring-dashboard.mjs generate",
    "docs:index:roles": "node scripts/master-index-automation.mjs roles",
    "docs:index:migration": "node scripts/master-index-automation.mjs migration",
    "docs:index:update": "node scripts/master-index-automation.mjs update",
    "docs:index:report": "node scripts/master-index-automation.mjs report",
