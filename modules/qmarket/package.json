{
  "name": "@anarq/qmarket",
  "version": "1.0.0",
  "description": "Qmarket - Content Marketplace Module for AnarQ&Q Ecosystem",
  "main": "src/index.js",
  "type": "module",
  "scripts": {
    "dev": "node src/index.js",
    "start": "NODE_ENV=production node src/index.js",
    "test": "vitest run",
    "test:watch": "vitest",
    "test:coverage": "vitest run --coverage",
    "test:unit": "vitest run tests/unit",
    "test:contract": "vitest run tests/contract",
    "test:integration": "vitest run tests/integration",
    "test:e2e": "vitest run tests/e2e",
    "lint": "eslint src tests",
    "lint:fix": "eslint src tests --fix",
    "build": "node scripts/build.js",
    "docker:build": "docker build -t qmarket .",
    "docker:run": "docker run -p 3008:3008 qmarket"
  },
  "dependencies": {
    "express": "^4.18.2",
    "cors": "^2.8.5",
    "helmet": "^7.1.0",
    "compression": "^1.7.4",
    "express-rate-limit": "^7.1.5",
    "joi": "^17.11.0",
    "uuid": "^9.0.1",
    "crypto": "^1.0.1",
    "node-fetch": "^3.3.2",
    "ws": "^8.14.2",
    "jsonwebtoken": "^9.0.2",
    "multer": "^1.4.5-lts.1",
    "mime-types": "^2.1.35"
  },
  "devDependencies": {
    "@anarq/common-schemas": "workspace:*",
    "@anarq/common-clients": "workspace:*",
    "@anarq/testing": "workspace:*",
    "vitest": "^1.0.4",
    "eslint": "^8.55.0",
    "supertest": "^6.3.3",
    "msw": "^2.0.11"
  },
  "keywords": [
    "marketplace",
    "content",
    "nft",
    "decentralized",
    "anarq",
    "q-ecosystem"
  ],
  "author": "AnarQ Team",
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "https://github.com/anarq/q-ecosystem.git",
    "directory": "modules/qmarket"
  },
  "engines": {
    "node": ">=18.0.0"
  }
}