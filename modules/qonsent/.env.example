# Environment
NODE_ENV=development
PORT=3003
HOST=0.0.0.0

# Database
MONGODB_URI=mongodb://localhost:27017/qonsent

# External Services
SQUID_API_URL=http://localhost:3001
QERBEROS_API_URL=http://localhost:3006
QLOCK_API_URL=http://localhost:3002
QINDEX_API_URL=http://localhost:3004

# Event Bus
EVENT_BUS_URL=redis://localhost:6379
EVENT_BUS_TYPE=redis

# Cache
REDIS_URL=redis://localhost:6379
CACHE_TTL=300

# Security
JWT_SECRET=your-secret-key-change-in-production
ENCRYPTION_KEY=your-encryption-key-32-chars-long

# Logging
LOG_LEVEL=info

# CORS
CORS_ORIGIN=*

# Rate Limiting
RATE_LIMIT_MAX=100
RATE_LIMIT_WINDOW=60000

# Feature Flags
ENABLE_UCAN_POLICIES=true
ENABLE_DELEGATION=true
ENABLE_AUDIT_LOGGING=true
ENABLE_REAL_TIME_REVOCATION=true