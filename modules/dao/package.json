{
  "name": "@anarq/dao",
  "version": "1.0.0",
  "description": "DAO/Communities governance module for the Q ecosystem",
  "main": "src/index.js",
  "type": "module",
  "scripts": {
    "dev": "NODE_ENV=development USE_MOCKS=true node src/index.js",
    "start": "node src/index.js",
    "start:integrated": "NODE_ENV=production USE_MOCKS=false node src/index.js",
    "test": "vitest run",
    "test:watch": "vitest",
    "test:coverage": "vitest run --coverage",
    "test:contract": "vitest run tests/contract/",
    "test:integration": "vitest run tests/integration/",
    "test:e2e": "vitest run tests/e2e/",
    "lint": "eslint src/ tests/",
    "lint:fix": "eslint src/ tests/ --fix",
    "validate:openapi": "swagger-codegen validate -i openapi.yaml",
    "validate:mcp": "ajv validate -s ../schemas/mcp.schema.json -d mcp.json"
  },
  "dependencies": {
    "@anarq/common-schemas": "^1.0.0",
    "@anarq/common-clients": "^1.0.0",
    "express": "^4.18.2",
    "cors": "^2.8.5",
    "helmet": "^7.0.0",
    "express-rate-limit": "^6.8.1",
    "joi": "^17.9.2",
    "jsonwebtoken": "^9.0.1",
    "crypto": "^1.0.1",
    "sqlite3": "^5.1.6",
    "node-cron": "^3.0.2",
    "axios": "^1.4.0"
  },
  "devDependencies": {
    "@anarq/testing": "^1.0.0",
    "vitest": "^0.34.0",
    "@vitest/coverage-v8": "^0.34.0",
    "eslint": "^8.45.0",
    "swagger-codegen-cli": "^3.0.0",
    "ajv-cli": "^5.0.0",
    "supertest": "^6.3.3"
  },
  "keywords": [
    "dao",
    "governance",
    "voting",
    "blockchain",
    "decentralized",
    "community",
    "proposals"
  ],
  "author": "AnarQ&Q Team",
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "https://github.com/anarq/q-ecosystem"
  }
}